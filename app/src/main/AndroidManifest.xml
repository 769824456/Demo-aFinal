<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.syl.demo" >

    <!-- aFinal所需权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FinalDB.FinalDBActivity"
            android:label="@string/title_activity_final_db" >
        </activity>
        <activity
            android:name=".FinalActivity.FinalActivityActivity"
            android:label="@string/title_activity_final" >
        </activity>
        <activity
            android:name=".FinalHttp.FinalHttpActivity"
            android:label="@string/title_activity_final_http" >
        </activity>
        <activity
            android:name=".FinalBitmap.FinalBitmapActivity"
            android:label="@string/title_activity_final_bitmap" >
        </activity>
    </application>

</manifest>
